def backtrack(state):
    if is_valid_solution(state):
        # 1. Base Case
        solutions.append(state.copy())  # Save solution
        return

    for choice in get_possible_choices(state):
        # 2. Make a choice
        state.add(choice)

        # 3. Recurse with new state
        backtrack(state)

        # 4. Undo choice (backtrack)
        state.remove(choice)